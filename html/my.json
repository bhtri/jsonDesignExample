{
    "type": "scaffold",
    "args": {
      "appBar": {
        "type": "app_bar",
        "args": {
          "title": {
            "type": "text",
            "args": {
              "text": "List of English words"
            }
          },
          "actions": [
            {
              "type": "icon_button",
              "args": {
                "icon": {
                  "codePoint": 59418,
                  "fontFamily": "MaterialIcons",
                  "matchTextDirection": true
                },
                "onPressed": "##click()##"
              }
            }
          ]
        }
      },
      "body": {
        "type": "center",
        "child": {
          "type": "circular_progress_indicator",
          "args": {
            "valueColor": "#f00"
          }
        }
      }
    }
  }